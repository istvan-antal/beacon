<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>xyz.istvan.beacon</string>
        <key>ProgramArguments</key>
        <array>
            <string>/usr/local/bin/node</string>
            <string>dist/app.js</string>
            <string>--pid-file=/Users/void/Developer/pids/beacon.pid</string>
        </array>
        <key>WorkingDirectory</key>
        <string>/Users/void/Developer/apps/beacon</string>
        <key>UserName</key>
        <string>void</string>
        <key>StandardOutPath</key>
        <string>/tmp/beacon-out.log</string>
        <key>StandardErrorPath</key>
        <string>/tmp/beacon-error.log</string>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>ThrottleInterval</key>
        <integer>30</integer>
    </dict>
</plist>